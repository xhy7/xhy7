<!-- 点赞容器 -->
<div class="like-container" id="like-<%= post.path %>">
    <button class="like-btn" onclick="toggleLike('<%= post.path %>')">
        <i class="iconfont icon-like"></i>
    </button>
    <span class="like-count" id="count-<%= post.path %>">0</span>
</div>

<!-- 引用Fluid内置图标库 -->
<%- css_ex(theme.static_prefix.icons, '/css/iconfont.css') %>